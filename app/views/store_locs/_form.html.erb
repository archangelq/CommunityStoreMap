<%= form_tag  storefind_url, :method => :post, :remote => true do %>
<% end %>

<%= form_for @store_loc do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :lat %><br />
    <%= f.text_field :lat %>
  </p>
  <p>
    <%= f.label :lng %><br />
    <%= f.text_field :lng %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :icon %><br />
    <%= f.text_field :icon %>
  </p>
  <p><%= f.submit %></p>
<% end %>
